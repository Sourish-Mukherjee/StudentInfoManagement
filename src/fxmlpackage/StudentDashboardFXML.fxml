<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@../csspackage/studentDashboard.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dashboard.StudentDashboardController">
   <children>
      <Pane id="pane" fx:id="pane" prefHeight="400.0" prefWidth="215.0">
         <children>
            <ImageView id="stuDashImage" fx:id="stuDashImage" fitHeight="174.0" fitWidth="171.0" layoutX="22.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/images/student_dashboard_icon.png" />
               </image>
            </ImageView>
            <Label id="stuDashEmail" fx:id="stuDashEmail" alignment="CENTER" layoutX="10.0" layoutY="40.0" prefHeight="26.0" prefWidth="195.0" text="Label" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
            <Label id="stuDashName" fx:id="stuDashName" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="304.0" prefHeight="26.0" prefWidth="195.0" text="Label" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
            <Label id="stuDashUSN" fx:id="stuDashUSN" alignment="CENTER" layoutX="10.0" layoutY="360.0" prefHeight="26.0" prefWidth="195.0" text="Label" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="293.0" layoutY="106.0" prefHeight="58.0" prefWidth="195.0" text="Dashboard" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Arial Bold" size="30.0" />
         </font>
      </Label>
      <Button id="stuDashAttend" fx:id="stuDashAttend" layoutX="258.0" layoutY="248.0" mnemonicParsing="false" onAction="#checkAttend" text="Check Attendance" />
      <Button id="stuDashMarks" fx:id="stuDashMarks" layoutX="443.0" layoutY="248.0" mnemonicParsing="false" onAction="#checkMarks" prefHeight="32.0" prefWidth="133.0" text="Check Marks" />
   </children>
</AnchorPane>
